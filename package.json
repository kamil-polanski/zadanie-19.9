{
    "name": "19.9",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "webpack",
        "test": "npm run",
        "init-project": "npm install && npm-run-all init:*",
        "init:dirs": "mkdirp sass css components",
        "init:files": "touch README.md index.html script.js sass/style.scss components/app.js components/user.js userlist.js",
        "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
        "build": "npm-run-all build:* test",
        "build:babel": "babel script.js --out-file build.js",
        "build:clean": "rm -rf css/*.css",
        "build:sass": "node-sass --output-style compact -o css sass",
        "build:autoprefixer": "autoprefixer-cli css/style.css",
        "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
        "watch": "npm-run-all build -p watch:*",
        "watch:browsersync": "browser-sync start --server --files css/*.css *.html",
        "watch:sassprefixer": "onchange -d 100 sass/*.scss -- npm-run-all build:sass build:autoprefixer",
        "watch:sass": "node-sass --output-style expanded --source-map true -o css sass --watch",
        "watch:autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.prefixed.css css/style.css"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "autoprefixer-cli": "^1.0.0",
        "babel-loader": "^8.0.6",
        "browser-sync": "^2.26.3",
        "globstar": "^1.0.0",
        "mkdirp": "^0.5.1",
        "node-sass": "^4.13.1",
        "npm-run-all": "^4.1.5",
        "nu-html-checker": "^0.1.0",
        "onchange": "^5.2.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "rimraf": "^2.6.3",
        "webpack": "^4.35.3",
        "babel-cli": "^6.26.0",
        "webpack-cli": "^3.3.6"
    },
    "dependencies": {},
    "repository": {
        "type": "git",
        "url": ""
    }
}